@page
@model Pages.Cars.List

@{
    Layout = null;
}

<div>
    <h1>Cars</h1>
    
    <table class="table">
        @foreach (var car in Model.Cars)
        {
            <tr>
                <td>@car.Make</td>
                <td>@car.Model</td>
                <td>@car.Colour</td>
                <td>@car.carType</td>
            </tr>
        }
    </table>
</div>